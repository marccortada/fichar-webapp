# Example environment variables for this project
# Copy this file to `.env.local` and fill in your values.
# IMPORTANT: Do NOT commit `.env.local` to Git (itâ€™s ignored in .gitignore).

# Public Supabase URL for your project
NEXT_PUBLIC_SUPABASE_URL=https://YOUR-PROJECT.supabase.co

# Public ANON key for your Supabase project (non-sensitive compared to service key)
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_ANON_KEY

# Service Role key (server-only). Never expose in client/browser code
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY

# Local dev server port
PORT=3000

# Public app URL used in client code (e.g., for redirects)
NEXT_PUBLIC_APP_URL=http://localhost:3000

